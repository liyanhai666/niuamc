page,
.container {
  width: 100%;
  height: 100%;
  background-color: #f5f5f5;
}

.container {
  justify-content: center;
  text-align: center;
}

.logo {
  margin: 0 auto;
  width: 222rpx;
  height: 222rpx;
}

.logo image {
  width: 100%;
  height: 100%;
}

.name {
  font-size: 46rpx;
  font-weight: 600;
  color: #333;
}

.btns {
  margin-top: 140rpx;
  justify-content: center;
}

.btns .btn {
  margin: 0;
  padding: 0;
  width: 260rpx;
  height: 76rpx;
  border-radius: 38rpx;
  background: linear-gradient(to right, #9a999a, #cccbcb);
  border: none;
  text-align: center;
  font-size: 28rpx;
  line-height: 76rpx;
  color: #fff;
}

.btns .auth {
  margin-right: 40rpx;
  background: linear-gradient(to right, #4c8aff, #76baff);
}